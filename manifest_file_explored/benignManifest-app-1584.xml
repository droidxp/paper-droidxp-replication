<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="6"
    android:versionName="5.5"
    package="com.tb.tube">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <uses-permission
        android:name="android.permission.INTERNET" />

    <uses-permission
        android:name="android.permission.GET_TASKS" />

    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:theme="@ref/0x7f0b005a"
        android:label="@ref/0x7f0a0010"
        android:icon="@ref/0x7f020078"
        android:allowBackup="true"
        android:hardwareAccelerated="true">

        <activity
            android:label="@ref/0x7f0a0010"
            android:name="com.mp3.youtube.activities.Splash"
            android:configChanges="0x4a0">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.mp3.youtube.activities.MainFragmentActivity"
            android:configChanges="0x4a0" />

        <activity
            android:name="com.android.music.MediaPlaybackActivity"
            android:configChanges="0x4a0" />

        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="0xfb0" />
    </application>
</manifest>
