<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="1"
    android:versionName="1.0"
    package="com.videosguideline">

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="10" />

    <application
        android:theme="@ref/0x7f050000"
        android:label="@ref/0x7f040000"
        android:icon="@ref/0x7f02000f"
        android:allowBackup="true">

        <activity
            android:name="SplashActivity"
            android:configChanges="0xa0">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="OpenYouTubePlayerActivity"
            android:screenOrientation="0"
            android:configChanges="0xa0" />

        <activity
            android:name="ListeAnnonces"
            android:configChanges="0xa0" />

        <activity
            android:name="Sample"
            android:configChanges="0xa0" />

        <activity
            android:name="Interstitial"
            android:configChanges="0xa0" />

        <activity
            android:name="InterstitialExit"
            android:configChanges="0xa0" />

        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="0x3b0" />
    </application>

    <uses-permission
        android:name="android.permission.INTERNET" />

    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission
        android:name="android.permission.ACCESS_WIFI_STATE" />
</manifest>
