<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="2"
    android:versionName="1.0.1"
    package="com.sikh.tube">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission
        android:name="android.permission.INTERNET" />

    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission
        android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission
        android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <uses-permission
        android:name="android.permission.GET_TASKS" />

    <uses-permission
        android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:theme="@ref/0x7f0b0056"
        android:label="@ref/0x7f0a000d"
        android:icon="@ref/0x7f020075"
        android:allowBackup="true">

        <activity
            android:label="@ref/0x7f0a000d"
            android:name="com.jakkash.hdvideo.SplashActivity">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.jakkash.hdvideo.MainActivity"
            android:configChanges="0x4a0" />

        <activity
            android:name="com.jakkash.hdvideo.CategoryItem"
            android:configChanges="0x4a0" />

        <activity
            android:name="com.jakkash.hdvideo.VideoPlay"
            android:configChanges="0x4a0" />

        <activity
            android:name="com.example.youtube.YoutubePlay"
            android:configChanges="0x4a0" />

        <activity
            android:name="com.jakkash.hdvideo.AboutActivity" />

        <activity
            android:theme="@ref/0x01030008"
            android:label="@ref/0x7f0a000d"
            android:name="com.example.play.OpenYouTubePlayerActivity"
            android:screenOrientation="0"
            android:configChanges="0x80" />

        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="0xfb0" />
    </application>
</manifest>
