<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="2"
    android:versionName="1.0.1"
    android:windowSoftInputMode="0x20"
    package="co.uk.pocketapp.rcpemobile">

    <compatible-screens>

        <screen
            android:screenSize="300"
            android:screenDensity="120" />

        <screen
            android:screenSize="300"
            android:screenDensity="160" />

        <screen
            android:screenSize="300"
            android:screenDensity="240" />

        <screen
            android:screenSize="300"
            android:screenDensity="320" />

        <screen
            android:screenSize="200"
            android:screenDensity="120" />

        <screen
            android:screenSize="200"
            android:screenDensity="160" />

        <screen
            android:screenSize="200"
            android:screenDensity="240" />
    </compatible-screens>

    <uses-permission
        android:name="android.permission.INTERNET" />

    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:label="@ref/0x7f050000"
        android:icon="@ref/0x7f020000"
        android:debuggable="true">

        <activity
            android:theme="@ref/0x01030009"
            android:label="@ref/0x7f050000"
            android:name="cordovaExample"
            android:screenOrientation="1"
            android:configChanges="0xb0">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="16" />
</manifest>
