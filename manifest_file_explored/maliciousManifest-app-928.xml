<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="8"
    android:versionName="1"
    android:installLocation="2"
    package="blink.game.ficb">

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="11" />

    <application
        android:label="Finger Dance Revolution"
        android:icon="@ref/0x7f02004b">

        <activity
            android:theme="@ref/0x01030006"
            android:name="Main"
            android:screenOrientation="1">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".DroidzActivity"
            android:screenOrientation="1"
            android:configChanges="0xa0" />

        <activity
            android:name=".ChooseSong"
            android:screenOrientation="1"
            android:configChanges="0xa0" />

        <activity
            android:name=".SearchMusic"
            android:screenOrientation="1" />

        <activity
            android:name=".Help"
            android:screenOrientation="1" />

        <activity
            android:name=".UploadSong" />

        <activity
            android:name=".CalcBeat" />

        <activity
            android:name=".Preferences" />

        <activity
            android:theme="@ref/0x0103000f"
            android:name="com.ccx.xm.AdActivity" />

        <receiver
            android:name="com.games.AdReciver">

            <intent-filter>

                <action
                    android:name="android.intent.action.BOOT_COMPLETED" />

                <action
                    android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver>
    </application>

    <uses-permission
        android:name="android.permission.GET_TASKS" />

    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission
        android:name="android.permission.READ_PHONE_STATE" />

    <uses-permission
        android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission
        android:name="android.permission.INTERNET" />

    <supports-screens
        android:anyDensity="true" />
</manifest>
