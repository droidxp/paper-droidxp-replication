<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="6"
    android:versionName="6.0"
    package="speed.boost.android.free">

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission
        android:name="android.permission.GET_TASKS" />

    <uses-permission
        android:name="android.permission.KILL_BACKGROUND_PROCESSES" />

    <uses-permission
        android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />

    <uses-permission
        android:name="android.permission.INTERNET" />

    <uses-permission
        android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:theme="@ref/0x7f080001"
        android:label="@ref/0x7f070000"
        android:icon="@ref/0x7f02000f"
        android:allowBackup="true">

        <activity
            android:label="@ref/0x7f070001"
            android:name="com.android.speedboosterpro.SpeedUp"
            android:screenOrientation="1">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.HOME" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="@ref/0x7f070005"
            android:name="com.android.speedboosterpro.ProcessInfo"
            android:screenOrientation="1" />

        <activity
            android:label="@ref/0x7f070008"
            android:name="com.android.speedboosterpro.BoostNow"
            android:screenOrientation="1">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="@ref/0x7f070009"
            android:name="com.android.speedboosterpro.About"
            android:screenOrientation="1" />

        <activity
            android:label="@ref/0x7f07000a"
            android:name="com.android.speedboosterpro.Settings"
            android:screenOrientation="1" />

        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="0xfb0" />

        <receiver
            android:name="com.android.speedboosterpro.AR"
            android:exported="true"
            android:priority="1000">

            <intent-filter>

                <action
                    android:name="android.intent.action.NOTIFY" />
            </intent-filter>
        </receiver>

        <service
            android:label="Auto Booster"
            android:name="com.android.speedboosterpro.AutoBooster" />
    </application>
</manifest>
