<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="7"
    android:versionName="1.6"
    package="org.anderdroid.quake3droid">

    <application
        android:label="@ref/0x7f040000"
        android:icon="@ref/0x7f020002"
        android:name="quake3droid">

        <activity
            android:label="@ref/0x7f040000"
            android:name="Launcher"
            android:launchMode="1"
            android:configChanges="0xb0">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="@ref/0x7f040000"
            android:name="Game"
            android:launchMode="2"
            android:configChanges="0xb0">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <supports-screens
            android:smallScreens="true"
            android:normalScreens="true"
            android:largeScreens="true" />

        <activity
            android:name="com.zeemote.zc.ui.android.ControllerAndroidUi$Activity" />
    </application>

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="8" />

    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission
        android:name="android.permission.INTERNET" />

    <uses-permission
        android:name="android.permission.BLUETOOTH" />

    <uses-permission
        android:name="android.permission.BLUETOOTH_ADMIN" />

    <uses-permission
        android:name="android.permission.WAKE_LOCK" />
</manifest>
