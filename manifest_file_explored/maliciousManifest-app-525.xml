    <application
        android:label="@ref/0x7f070000"
        android:icon="@ref/0x7f020011">

        <activity
            android:label="@ref/0x7f070000"
            android:name=".GLActivity">

            <intent-filter>

                <action
                    android:name="android.intent.action.MAIN" />

                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="@ref/0x7f070000"
